
{
  "name": "folder_uploads",
  "title": "文件夹上传记录",
  "schemas": [
    {
      "name": "userId",
      "title": "用户ID",
      "type": "String",
      "required": true
    },
    {
      "name": "folderName",
      "title": "文件夹名",
      "type": "String",
      "required": true
    },
    {
      "name": "folderPath",
      "title": "文件夹路径",
      "type": "String",
      "required": true
    },
    {
      "name": "fileCount",
      "title": "文件数量",
      "type": "Number"
    },
    {
      "name": "totalSize",
      "title": "总大小(bytes)",
      "type": "Number"
    },
    {
      "name": "folderStructure",
      "title": "文件夹结构",
      "type": "Array",
      "items": {
        "type": "Object",
        "format": {
          "name": {
            "type": "String",
            "title": "文件名"
          },
          "type": {
            "type": "String",
            "title": "文件类型"
          },
          "path": {
            "type": "String",
            "title": "文件路径"
          }
        }
      }
    },
    {
      "name": "isProjectFolder",
      "title": "是否项目文件夹",
      "type": "Boolean"
    }
  ]
}
  