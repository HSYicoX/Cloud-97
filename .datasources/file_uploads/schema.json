
{
  "name": "file_uploads",
  "title": "文件上传记录",
  "schemas": [
    {
      "name": "userId",
      "title": "用户ID",
      "type": "String",
      "required": true
    },
    {
      "name": "fileName",
      "title": "文件名",
      "type": "String",
      "required": true
    },
    {
      "name": "fileType",
      "title": "文件类型",
      "type": "String"
    },
    {
      "name": "fileSize",
      "title": "文件大小(Bytes)",
      "type": "Number"
    },
    {
      "name": "filePath",
      "title": "文件存储路径",
      "type": "String",
      "required": true
    },
    {
      "name": "hash",
      "title": "文件哈希值",
      "type": "String"
    },
    {
      "name": "isPublic",
      "title": "是否公开",
      "type": "Boolean"
    },
    {
      "name": "downloadCount",
      "title": "下载次数",
      "type": "Number"
    },
    {
      "name": "description",
      "title": "文件描述",
      "type": "MultiLineString"
    },
    {
      "name": "tags",
      "title": "标签",
      "type": "Array",
      "items": {
        "type": "String"
      }
    },
    {
      "name": "expireTime",
      "title": "过期时间",
      "type": "DateTime",
      "format": "timestamp-ms"
    }
  ]
}
  