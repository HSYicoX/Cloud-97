
{
  "name": "file_uploads",
  "title": "文件上传记录",
  "schemas": [
    {
      "name": "userId",
      "title": "用户ID",
      "type": "String",
      "required": true
    },
    {
      "name": "fileName",
      "title": "文件名",
      "type": "String",
      "required": true
    },
    {
      "name": "fileType",
      "title": "文件类型",
      "type": "String",
      "required": true
    },
    {
      "name": "fileSize",
      "title": "文件大小(bytes)",
      "type": "Number",
      "required": true
    },
    {
      "name": "filePath",
      "title": "文件存储路径",
      "type": "String",
      "required": true
    },
    {
      "name": "fileHash",
      "title": "文件哈希值",
      "type": "String"
    },
    {
      "name": "uploadType",
      "title": "上传类型",
      "type": "String",
      "enum": ["file", "code", "folder", "archive"],
      "required": true
    },
    {
      "name": "isPublic",
      "title": "是否公开",
      "type": "Boolean",
      "default": false
    },
    {
      "name": "metadata",
      "title": "元数据",
      "type": "Object",
      "format": {
        "codeLanguage": {
          "type": "String",
          "title": "编程语言(代码文件)"
        },
        "fileCount": {
          "type": "Number",
          "title": "文件数量(文件夹/压缩包)"
        },
        "compressionFormat": {
          "type": "String",
          "title": "压缩格式(压缩包)"
        },
        "repositoryId": {
          "type": "String",
          "title": "关联仓库ID(代码文件)"
        }
      }
    },
    {
      "name": "status",
      "title": "状态",
      "type": "String",
      "enum": ["uploading", "processing", "completed", "failed"],
      "required": true
    },
    {
      "name": "errorMessage",
      "title": "错误信息",
      "type": "String"
    },
    {
      "name": "expiryTime",
      "title": "过期时间",
      "type": "DateTime",
      "format": "timestamp-ms"
    }
  ]
}
  