
{
  "name": "file_uploads",
  "title": "文件上传记录",
  "schemas": [
    {
      "name": "userId",
      "title": "用户ID",
      "type": "String",
      "required": true
    },
    {
      "name": "fileName",
      "title": "文件名",
      "type": "String",
      "required": true
    },
    {
      "name": "fileType",
      "title": "文件类型",
      "type": "String"
    },
    {
      "name": "fileSize",
      "title": "文件大小(bytes)",
      "type": "Number"
    },
    {
      "name": "filePath",
      "title": "文件存储路径",
      "type": "String",
      "required": true
    },
    {
      "name": "thumbnailPath",
      "title": "缩略图路径",
      "type": "String"
    },
    {
      "name": "isPublic",
      "title": "是否公开",
      "type": "Boolean",
      "default": false
    },
    {
      "name": "uploadMethod",
      "title": "上传方式",
      "type": "String"
    },
    {
      "name": "ipAddress",
      "title": "上传IP地址",
      "type": "String"
    },
    {
      "name": "metadata",
      "title": "文件元数据",
      "type": "Object"
    }
  ]
}
  