
{
  "name": "archive_uploads",
  "title": "压缩包上传记录",
  "schemas": [
    {
      "name": "userId",
      "title": "用户ID",
      "type": "String",
      "required": true
    },
    {
      "name": "fileName",
      "title": "文件名",
      "type": "String",
      "required": true
    },
    {
      "name": "filePath",
      "title": "文件路径",
      "type": "String",
      "required": true
    },
    {
      "name": "archiveType",
      "title": "压缩类型",
      "type": "String"
    },
    {
      "name": "fileSize",
      "title": "文件大小(bytes)",
      "type": "Number"
    },
    {
      "name": "compressionRatio",
      "title": "压缩率",
      "type": "Number"
    },
    {
      "name": "fileCount",
      "title": "包含文件数",
      "type": "Number"
    },
    {
      "name": "extractedPath",
      "title": "解压路径",
      "type": "String"
    },
    {
      "name": "isExtracted",
      "title": "是否已解压",
      "type": "Boolean"
    },
    {
      "name": "passwordProtected",
      "title": "是否有密码保护",
      "type": "Boolean"
    }
  ]
}
  