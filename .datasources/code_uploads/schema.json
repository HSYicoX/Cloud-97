
{
  "name": "code_uploads",
  "title": "代码文件上传记录",
  "schemas": [
    {
      "name": "userId",
      "title": "用户ID",
      "type": "String",
      "required": true
    },
    {
      "name": "repoId",
      "title": "关联仓库ID",
      "type": "String"
    },
    {
      "name": "fileName",
      "title": "文件名",
      "type": "String",
      "required": true
    },
    {
      "name": "filePath",
      "title": "文件路径",
      "type": "String",
      "required": true
    },
    {
      "name": "language",
      "title": "编程语言",
      "type": "String"
    },
    {
      "name": "fileSize",
      "title": "文件大小(bytes)",
      "type": "Number"
    },
    {
      "name": "isExecutable",
      "title": "是否可执行",
      "type": "Boolean"
    },
    {
      "name": "lineCount",
      "title": "代码行数",
      "type": "Number"
    },
    {
      "name": "characterCount",
      "title": "字符数",
      "type": "Number"
    },
    {
      "name": "licenseType",
      "title": "许可证类型",
      "type": "String"
    },
    {
      "name": "dependencies",
      "title": "依赖项",
      "type": "Array",
      "items": {
        "type": "String"
      }
    }
  ]
}
  